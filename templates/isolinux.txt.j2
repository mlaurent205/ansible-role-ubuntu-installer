default preseed
label preseed
  menu label ^Auto Install Ubuntu Server
  kernel /install/vmlinuz
{% if preseed_url != '' %}
  append preseed/url={{ preseed_file}} auto=true priority=high vga=788 initrd=/install/initrd.gz quiet ---
{% else %}
  append preseed/file=/cdrom/{{ preseed_file}} preseed/file/checksum={{ preseed_checksum }} auto=true priority=high vga=788 initrd=/install/initrd.gz quiet ---
{% endif %}
